{
    "name": "willy68/pgconsole",
    "description": "Application console pour pgframework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "William Lety",
            "email": "william.lety@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^6.0",
        "psr/container": "^2.0",
        "symfony/dotenv": "^6.0",
        "doctrine/orm": "^2.11",
        "doctrine/migrations": "^3.5",
        "doctrine/data-fixtures": "^1.5",
        "willy68/pgmodule": "^1.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": ["bin/console"],
    "autoload": {
        "psr-4": {
            "Application\\Console\\": "src"
        }
    }
}
